(def
  sum (fn (n) 
        (
          (def inner 
            (fn (n acc) 
              (if (= n 0) 
                acc   
                (inner (- n 1) (+ n acc)))))
          n 0)))

(defn fizz-buzz (start end)
  (subvec 
    (lazy-vector 
      #(cond 
        (= 0 (rem % 15)) "fizzbuzz" 
        (= 0 (rem % 3)) "fizz" 
        (= 0 (rem % 5)) "buzz"
        :else %)) 
    start end))

