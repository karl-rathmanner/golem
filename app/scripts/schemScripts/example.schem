(defn fizz-buzz (start end)
  (subvec 
    (lazy-vector 
      #(cond 
        (= 0 (rem % 15)) "fizzbuzz" 
        (= 0 (rem % 3)) "fizz" 
        (= 0 (rem % 5)) "buzz"
        :else %)) 
    start end))

(defn add1 (x) (+ x 1))
(defmacro executePostfix (fn (l) (cons (last l) (butlast l))))

